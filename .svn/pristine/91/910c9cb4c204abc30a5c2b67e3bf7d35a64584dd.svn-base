<div class="rightDiv">
    <div class="k-block">
        <h3 class="k-header"> Dealer Details</h3>



        <div id="DealerInformationDiv" class="divDetails">

            <div class="leftchildDiv">
                @*<div class="k-block">

                    <h3 class="k-header"> Location Summary</h3>*@

                <div id="LocationSummaryDiv" class="divSummary">



                    @*</div>*@
                </div>


            </div>
            <ul>
                <li>
                    <input id="hdnDealerId" Type="hidden" value="0" />
                </li>
                <li>
                    <label class="lbl widthSize20_per">Dealer Name:</label>
                    <input type="text" class="k-textbox" id="txtDealerName" name="DealerName" title="Please Select Your Name" required="required" />


                </li>


                <li>
                    <label class="lbl widthSize20_per">Dealer Code:</label>
                    <input type="text" class="k-textbox" id="txtDealerCode" name="DealerCode" title="Please Select Your Name" required="required" />

                </li>


                <li>
                    <label class="lbl widthSize20_per">Mobile No:</label>
                    <input type="text" class="k-textbox" id="txtMobileNo" name="MobileNo" title="Please Select Your Name" required="required" />

                </li>
                <li>
                    <label class="lbl widthSize20_per">Email Address:</label>
                    <input type="text" class="k-textbox" id="txtEmailAddress" name="EmailAddress" title="Please Select Your Name" required="required" />

                </li>
                <li>
                    <input id="FileAttachment" type="hidden" />
                    <a id="lnkAttachment" href="" target="_blank">View File</a>
                    <label class="lbl widthSize20_per">Agrement File:</label>
                    @*<input type="file" class="k-textbox" id="txtAgrementFile" name="AgrementFile" title="Please Select Your Name" required="required" />*@
                    <input name="files" id="txtAgrementFile" type="file" aria-label="files" />
                </li>
                <li>
                    <label for="txtDealerTypeName" class="lbl widthSize20_per">Dealer Type Name:</label>
                    <input id="txtDealerTypeName" required title="Please enter valid DealerType Name" />

                </li>
                <li class="accept">
                    <button id="btnSave" class="k-button k-primary  " type="submit">Submit</button>
                    &nbsp;&nbsp;
                    <button id="btnClearAll" class="k-button k-primary">Clear All</button>
                </li>
                <li class="status"></li>








            </ul>


        </div>


    </div>

</div>